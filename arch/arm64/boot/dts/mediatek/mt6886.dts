			#size-cells = <0>;

			scmi_tinysys: protocol@80 {
				reg = <0x80>;
				scmi-qos = <1>;
				scmi-met = <3>;
				scmi-apmcupm = <4>;
				scmi-mminfra = <5>;
				scmi-gpupm = <6>;
				scmi-plt = <7>;
				scmi_smi = <8>;
				scmi-cm = <9>;
				scmi-slbc = <10>;
				scmi_ssc = <11>;
			};
		};
	};


